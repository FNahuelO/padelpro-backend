{
  "version": 2,
  "buildCommand": "npx prisma generate && npx nest build",
  "installCommand": "npm install",
  "functions": {
    "api/index.js": {
      "includeFiles": ["dist/**", "node_modules/.prisma/**", "node_modules/@prisma/client/**"]
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/api" }
  ]
}
